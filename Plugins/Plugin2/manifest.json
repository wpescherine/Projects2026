{
  "manifest_version": 3,
  "name": "Timekeepr Tracker",
  "version": "2.8",
  "description": "Sample Timekeeper Tracker Plugin",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/Clock16.png",
      "48": "icons/Clock48.png"
    }
  },
  "icons": {
    "128": "icons/Clock128.png"
  }
}